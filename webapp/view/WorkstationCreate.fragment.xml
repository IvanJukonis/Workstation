<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
<Dialog
        id="workstationDialog"
        class="wsCreate"
        title="Guardar Estación de Trabajo">
    <VBox>
        <HBox class="wsRow centerDeposito">
            <Label text="Depósito:" width="6rem"/>
            <Input id="inputDeposito"
                   value="{info>/DepositoId}"
                   placeholder="Ingrese nombre del depósito" />
        </HBox>
    </VBox>
    <beginButton>
        <Button text="Continuar" press="onContinuarDeposito" />
    </beginButton>
</Dialog>
</core:FragmentDefinition>