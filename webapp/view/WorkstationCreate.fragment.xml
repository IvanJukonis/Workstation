<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="workstationDialog" title="Crear Registro">
        <VBox>
            <Input id="input1" placeholder="Ingrese valor 1"/>
            <Input id="input2" placeholder="Ingrese valor 2"/>
            <Input id="input3" placeholder="Ingrese valor 3"/>
            <Input id="input4" placeholder="Ingrese valor 4"/>
        </VBox>
        <beginButton>
            <Button text="Crear" press="onCreateDialog"/>
        </beginButton>
        <endButton>
            <Button text="Cancelar" press="onCloseDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>